{
  "manifest_version": 3,
  "name": "Many Tabs",
  "version": "1.0",
  "description": "Keep track of the number of open tabs and windows",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
